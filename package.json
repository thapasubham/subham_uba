{
  "name": "uba-ims",
  "version": "1.0.0",
  "type": "module",
  "description": "CRUD Application",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node --env-file=.env dist/src/index.js",
    "dev": "tsx watch --env-file=.env  src/index.ts ",
    "lint": "eslint",
    "typeorm": "typeorm-ts-node-esm",
    "test": "c8  mocha src/tests/**/*.test.ts ",
    "migration:run": "npm run typeorm -- migration:run -d ./src/data-source.ts",
    "migration:gen": "npm run typeorm -- migration:generate -d ./src/data-source.ts ./src/migrations/"
  },
  "author": "Subham Thapa",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/chai": "^5.2.2",
    "@types/express": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/mysql": "^2.15.27",
    "@types/node": "^22.15.17",
    "@types/sinon": "^17.0.4",
    "chai": "^5.2.0",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "mocha": "^11.2.2",
    "sinon": "^20.0.0",
    "supertest": "^7.1.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "dependencies": {
    "@apollo/server": "^4.12.0",
    "chalk": "^5.4.1",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.23"
  }
}
